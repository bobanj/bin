alias lic="cat confluence-not-shippable/src/main/resources/test-data.properties | ruby -e 'i = 0; print \$stdin.read.split(/\n\n/).grep(/CONF_COMM=/).first.gsub(/CONF_COMM=/, \"\").gsub(/^#.*\n/,\"\").strip' | pbcopy"


