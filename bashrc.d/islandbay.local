alias team="mvim $HOME/Dropbox/work/teamlead/team/TeamNotes.md"
alias key="mvim $HOME/Dropbox/Keyboard-Shortcuts.md"
alias tail_eac="ssh -l ssaasen eac.private.atlassian.com 'tail -F /opt/j2ee/domains/atlassian.com/extranet/confluence-home/logs/atlassian-confluence'"
alias tail_cac="ssh -l ssaasen confluence.atlassian.com 'tail -F /opt/j2ee/domains/atlassian.com/confluence/webapps/atlassian-confluence/data/logs/atlassian-confluence.log'"
